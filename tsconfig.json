{
  "compilerOptions": {
    "target": "es5",
    "lib": ["dom", "dom.iterable", "esnext"],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": true,
    "noEmit": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "bundler",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "incremental": true,
    "plugins": [
      {
        "name": "next"
      }
    ],
    "paths": {
      "@/*": ["./src/*"]
    }
  },
  "include": [
    "next-env.d.ts",
    "**/*.ts",
    "**/*.tsx",
    ".next/types/**/*.ts",
    "src/components/TreeForm.jsx",
    "src/app/ui/map/page.jtx",
    "src/app/lib/action.ts",
    "src/context/LocationContext.tsx",
    "src/app/api/index.tsx",
    "src/app/api/index.ts",
    "src/app/api/index.ts",
    "src/app/ui/form/From.jsx",
    "src/app/api/route.js",
    "src/app/ui/form/Form.tsx",
    "src/components/Map2.jsx"
  ],
  "exclude": ["node_modules"]
}
